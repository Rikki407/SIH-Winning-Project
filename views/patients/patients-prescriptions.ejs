                <div class="col-xs-12 ">
                    <div class="add-header-wrapper gradient-blue curved-section text-center">
                        <h2 class="uppercase bold w-text">Patient Prescriptions</h2>
                        <div class="before-text">Patient Prescriptions</div>
                        <p class="g-text">Patients' prescriptions ordered by the latest</p>
                    </div>

                    <div class=" bg-w">
                        <!-- <form action="/view-prescription" method="get"> -->
                            <div class="col-lg-10 col-lg-offset-1 col-xs-12">
                            <section class="box ">
                              <table class="table table-hover">
                              <thead>
                                <tr>
                                  <th scope="col">Prescription Id</th>
                                  <th scope="col">Doctor Name</th>
                                  <th scope="col">Date of Appointment</th>
                                </tr>
                              </thead>
                              <tbody>

                                <%pres.forEach((p,i)=>{ %>
                                <tr>
                                  <th scope="row"><%=i%></th>
                                  <td>Dr. <%=p.doctor_name%></td>
                                  <td><%=moment.utc(p.created).utcOffset("+05:30").format("LLLL")%></td>
                                  <td><a href="/view-prescription/<%=p._id%>" class="btn btn-outline-primary">View Prescription</a></td>
                                </tr>
                                <%})%>
                               
                              </tbody>
                            </table>
                            </div>
                        <!-- </form> -->




                    </div>
                </div>




